@model  List<Model.EF.Menu>
<div class="call">
    <p><span>Hỗ trợ</span> gọi <span class="number">01656-145-299</span></p>
</div>
<div class="account_desc">
    @if (string.IsNullOrEmpty(HttpContext.Current.User.Identity.Name))
    {
        <ul>
            @foreach (var item in Model)
            {
                <li><a href="@item.Link" target="@item.Target">@item.Text</a></li>
            }

        </ul>
    }
    else
    {
        <ul>
            
                <li>@ViewData["FullName"]</li>
            <li><a href="@Url.Action("Logout","Account")" >Thoát</a></li>
            
        </ul>
    }
    
</div>
<div class="clear"></div>
